:plain
  $('.mattrab-feed').append('#{ j render :partial => "feed", :collection => @feeds}');
  $('.pagination-nav').html('#{ j will_paginate @feeds, renderer: WillPaginate::ActionView::BootstrapLinkRenderer, list_classes: %w(pagination justify-content-center), previous_label: '&laquo;', next_label: '&raquo;', inner_window: 1, outer_window: 0 }');
